{
    "name": "ironlady/php-telegram-bot", 
    "version": "0.1.0",
    "type": "library",
    "description": "PHP telegram bot",
    "keywords": ["telegram", "bot", "api"],
    "license": "MIT",
    "homepage": "https://github.com/IronLady/php-telegram-bot",
    "authors": [
        {
            "name": "Samuel Sena",
            "email": "sammy171288@gmail.com"
        }
    ],
   "repositories": [
        {
            "type": "git",
            "url": "https://github.com/IronLady/firebase-php"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "monolog/monolog": "^1.19",
        "guzzlehttp/guzzle": "~6.0", 
        "ironlady/firebase-php": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Longman\\TelegramBot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.5"
    }
}
